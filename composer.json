{
  "name": "hda-typo3/hda-elements",
  "type": "typo3-cms-extension",
  "description": "Hda Elements Installation",
  "authors": [
    {
      "name": "Michael Lang",
      "role": "Developer",
      "email": "michael.lang@h-da.de"
    }
  ],
  "license": ["GPL-2.0-or-later"],
  "keywords": [
    "TYPO3",
    "extension",
    "hda"
  ],
  "repositories": [
    {
      "type": "typo3-cms-extension",
      "package": {
        "name": "hda-typo3/hda_elements",
        "source": {
          "type" : "git",
          "url" : "https://github.com/hda-typo3/hda_elements.git",
          "reference" : "main"
        },
        "dist": {
          "url": "https://github.com/hda-typo3/hda_elements/archive/refs/heads/main.zip",
          "type": "zip"
        }
      }
    }
  ],
  "require": {
    "typo3/cms-core": "^10.4 || ^11.5"
  },
  "autoload": {
    "psr-4": {
        "Hda\\HdaElements\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "hda_elements"
    }
  }
}
